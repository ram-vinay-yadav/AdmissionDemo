# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'


def commonPods
  
  #Networking
  pod 'Alamofire', '~> 5.0.5'
  #Local database
  pod 'SQLite.swift', '~> 0.11.5'
  #Keyboard manage
  pod 'IQKeyboardManagerSwift'
  #Animation
  pod "SkeletonView"
  
end


target 'AddmissionDemoRam' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for AddmissionDemoRam
commonPods

end
